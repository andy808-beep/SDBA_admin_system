{
  "buildCommand": "node scripts/inject-env.js && node scripts/inject-version.js",
  "outputDirectory": "public",
  "installCommand": "echo 'No dependencies to install'",
  "framework": null,
  "rewrites": [
    {
      "source": "/tn",
      "destination": "/register.html?e=tn"
    },
    {
      "source": "/wu",
      "destination": "/register.html?e=wu"
    },
    {
      "source": "/sc",
      "destination": "/register.html?e=sc"
    },
    {
      "source": "/",
      "destination": "/register.html"
    }
  ],
  "headers": [
    {
      "source": "/js/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600, must-revalidate"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}

